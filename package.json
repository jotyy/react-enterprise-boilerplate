    "eslint-plugin-storybook": "^0.6.15",
    "eslint-plugin-tailwindcss": "^3.13.1",
    "happy-dom": "^12.10.3",
    "husky": "^8.0.3",
    "lucide-react": "^0.307.0",
    "postcss": "^8.4.33",
    "prettier": "^3.1.1",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "storybook": "^7.6.7",
    "tailwindcss": "^3.4.1",
    "tailwindcss-animate": "^1.0.7",
